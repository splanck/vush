#!/usr/bin/env expect
set timeout 5
spawn ../vush
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "trap 'echo trapped' INT\r"
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
set pid [exp_pid]
exec kill -INT $pid
expect {
    -re "\[\r\n\]+trapped\[\r\n\]+vush> " {}
    timeout { send_user "trap output mismatch\n"; exit 1 }
}
send "exit\r"
expect {
    eof {}
    timeout { send_user "eof timeout\n"; exit 1 }
}
