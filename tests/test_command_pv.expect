#!/usr/bin/env expect
set timeout 5
spawn ../vush
expect "vush> "
send "export PATH=/tmp\r"
expect "vush> "
send "command -pv ls\r"
expect {
    -re "\[\r\n\]+.*/ls\[\r\n\]+vush> " {}
    timeout { send_user "command -pv output mismatch\n"; exit 1 }
}
send "echo \$?\r"
expect {
    -re "\[\r\n\]+0\[\r\n\]+vush> " {}
    timeout { send_user "command -pv status mismatch\n"; exit 1 }
}
send "exit\r"
expect eof
