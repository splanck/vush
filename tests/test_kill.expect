#!/usr/bin/env expect
set timeout 5
spawn ../vush
expect "vush> "
send "sleep 5 &\r"
expect "vush> "
send "kill 1\r"
expect {
    -re "\[vush\] job [0-9]+ finished[\r\n]+vush> " {}
    timeout { send_user "kill output mismatch\n"; exit 1 }
}
send "jobs\r"
expect {
    -re "vush> " {}
    -re "\[1\]" { send_user "job not terminated\n"; exit 1 }
    timeout { send_user "jobs timeout\n"; exit 1 }
}
send "exit\r"
expect eof
