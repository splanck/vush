#!/usr/bin/expect -f
set timeout 5
spawn ../vush
expect "vush> "
send "alias ll='echo hi'\r"
expect "vush> "
send "alias\r"
expect {
    -re "[\r\n]+ll='echo hi'[\r\n]+vush> " {}
    timeout { send_user "alias listing failed\n"; exit 1 }
}
send "ll there\r"
expect {
    -re "[\r\n]+hi there[\r\n]+vush> " {}
    timeout { send_user "alias use failed\n"; exit 1 }
}
send "unalias ll\r"
expect "vush> "
send "alias\r"
expect "vush> "
send "exit\r"
expect eof
