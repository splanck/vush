#!/usr/bin/env expect
set timeout 5
spawn ../vush
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "kill -l\r"
expect {
    -re "TERM" {}
    timeout { send_user "kill -l output mismatch\n"; exit 1 }
}
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "exit\r"
expect {
    eof {}
    timeout { send_user "eof timeout\n"; exit 1 }
}
