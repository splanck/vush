#!/usr/bin/env expect
set timeout 5
spawn [file dirname [info script]]/../vush
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "cat <<<\"hello\"\r"
expect {
    -re "\[\r\n\]+hello\[\r\n\]+vush> " {}
    timeout { send_user "here-string failed\n"; exit 1 }
}
send "exit\r"
expect {
    eof {}
    timeout { send_user "eof timeout\n"; exit 1 }
}
