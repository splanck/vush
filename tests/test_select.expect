#!/usr/bin/expect -f
set timeout 5
spawn ../vush
expect "vush> "
send "select x in foo bar; do echo $x; break; done\r"
expect {
    -re "1\\) foo[\r\n]+2\\) bar[\r\n]+\\? " {}
    timeout { send_user "select menu failed\n"; exit 1 }
}
send "2\r"
expect {
    -re "bar[\r\n]+vush> " {}
    timeout { send_user "select output mismatch\n"; exit 1 }
}
send "exit\r"
expect eof

