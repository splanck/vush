#!/usr/bin/env expect
set timeout 5
spawn [file dirname [info script]]/../vush -c {echo -n hi}
expect {
    "hi" {}
    eof {}
    timeout { send_user "echo -n output mismatch\n"; exit 1 }
}
spawn [file dirname [info script]]/../vush -c {echo -e hi}
expect {
    -re "hi\r?\n" {}
    eof {}
    timeout { send_user "echo -e output mismatch\n"; exit 1 }
}
