#!/usr/bin/env expect
set timeout 5
spawn ../vush -c {echo -n hi}
expect {
    "hi" {}
    timeout { send_user "echo -n output mismatch\n"; exit 1 }
}
expect {
    eof {}
    timeout { send_user "eof timeout\n"; exit 1 }
}
spawn ../vush -c {echo -e hi}
expect {
    -re "hi\r?\n" {}
    timeout { send_user "echo -e output mismatch\n"; exit 1 }
}
expect {
    eof {}
    timeout { send_user "eof timeout\n"; exit 1 }
}
