#!/usr/bin/env expect
set timeout 5
spawn ../vush
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "echo test >redir.tmp\r"
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "cat redir.tmp\r"
expect {
    -re "\[\r\n\]+test\[\r\n\]+vush> " {}
    timeout { send_user "redirection failed\n"; exit 1 }
}
send "rm redir.tmp\r"
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "exit\r"
expect {
    eof {}
    timeout { send_user "eof timeout\n"; exit 1 }
}
