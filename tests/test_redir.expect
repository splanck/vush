#!/usr/bin/env expect
set timeout 5
spawn ../vush
expect "vush> "
send "echo test >redir.tmp\r"
expect "vush> "
send "cat redir.tmp\r"
expect {
    -re "\[\r\n\]+test\[\r\n\]+vush> " {}
    timeout { send_user "redirection failed\n"; exit 1 }
}
send "rm redir.tmp\r"
expect "vush> "
send "exit\r"
expect eof
