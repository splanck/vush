#!/usr/bin/env expect
set timeout 5
spawn ../vush
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "for x in a b c; do echo \$x; done\r"
expect {
    -re "a\[\r\n\]+b\[\r\n\]+c\[\r\n\]+vush> " {}
    timeout { send_user "for output mismatch\n"; exit 1 }
}
send "exit\r"
expect {
    eof {}
    timeout { send_user "eof timeout\n"; exit 1 }
}
