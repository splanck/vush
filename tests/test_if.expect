#!/usr/bin/env expect
set timeout 5
spawn ../vush
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "if test 1 -eq 1; then echo ok; else echo fail; fi\r"
expect {
    -re "ok\[\r\n\]+vush> " {}
    timeout { send_user "if output mismatch\n"; exit 1 }
}
send "exit\r"
expect {
    eof {}
    timeout { send_user "eof timeout\n"; exit 1 }
}
