#!/usr/bin/expect -f
set timeout 5
spawn ../vush
expect "vush> "
send "foo() { echo hi \$1; }\r"
expect "vush> "
send "foo there\r"
expect {
    -re "[\r\n]+hi there[\r\n]+vush> " {}
    timeout { send_user "function output failed\n"; exit 1 }
}
send "bar() { return 7; }\r"
expect "vush> "
send "bar\r"
expect "vush> "
send "echo $?\r"
expect {
    -re "[\r\n]+7[\r\n]+vush> " {}
    timeout { send_user "return status failed\n"; exit 1 }
}
send "exit\r"
expect eof
