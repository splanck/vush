#!/usr/bin/env expect
set timeout 5
spawn ../vush
expect "vush> "
send "pushd /tmp\r"
expect {
    -re "\[\r\n\]+$env(PWD)\[\r\n\]+vush> " {}
    timeout { send_user "pushd output mismatch\n"; exit 1 }
}
send "dirs\r"
expect {
    -re "\[\r\n\]+$env(PWD)\[\r\n\]+vush> " {}
    timeout { send_user "dirs output mismatch\n"; exit 1 }
}
send "popd\r"
expect {
    -re "\[\r\n\]+vush> " {}
    timeout { send_user "popd output mismatch\n"; exit 1 }
}
send "exit\r"
expect eof
