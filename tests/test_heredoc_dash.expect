#!/usr/bin/env expect
set timeout 5
spawn [file dirname [info script]]/../build/vush
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "cat <<-EOF\r"
send "\tfoo\r"
send "EOF\r"
expect {
    -re "\[\r\n\]+foo\[\r\n\]+vush> " {}
    timeout { send_user "heredoc <<- failed\n"; exit 1 }
}
send "exit\r"
expect {
    eof {}
    timeout { send_user "eof timeout\n"; exit 1 }
}
