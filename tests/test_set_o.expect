#!/usr/bin/env expect
set timeout 5
spawn [file dirname [info script]]/../build/vush
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "set -o\r"
expect {
    -re {pipefail\s+off} {}
    timeout { send_user "set -o listing missing\n"; exit 1 }
}
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "exit\r"
expect {
    eof {}
    timeout { send_user "eof timeout\n"; exit 1 }
}
