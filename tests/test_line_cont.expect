#!/usr/bin/env expect
set timeout 5
set script [exec mktemp]
set f [open $script "w"]
puts $f {echo one \\}
puts $f {two}
close $f
spawn [file dirname [info script]]/../vush $script
expect {
    -re "one two\r?\n" {}
    timeout { send_user "line continuation failed\n"; exec rm $script; exit 1 }
}
expect {
    eof {}
    timeout { send_user "eof timeout\n"; exit 1 }
}
exec rm $script
