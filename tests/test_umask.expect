#!/usr/bin/expect -f
set timeout 5
spawn ../vush
expect "vush> "
send "umask\r"
expect {
    -re "[\r\n]+([0-7]{4})[\r\n]+vush> " { set old $expect_out(1,string) }
    timeout { send_user "umask read failed\n"; exit 1 }
}
send "umask 077\r"
expect "vush> "
send "umask\r"
expect {
    -re "[\r\n]+0077[\r\n]+vush> " {}
    timeout { send_user "umask set failed\n"; exit 1 }
}
send "umask $old\r"
expect "vush> "
send "exit\r"
expect eof
