#!/usr/bin/expect -f
set timeout 5
spawn ../vush
expect "vush> "
send "cat <<EOF\r"
send "hello\r"
send "world\r"
send \004
expect {
    -re "syntax error: here-document delimited by end-of-file" {}
    timeout { send_user "unterminated heredoc not detected\n"; exit 1 }
}
expect "vush> "
send "exit\r"
expect eof
