#!/usr/bin/env expect
set timeout 5
spawn ../vush
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "cat <<EOF\r"
send "hello\r"
send "world\r"
send \004
expect {
    -re "syntax error: here-document delimited by end-of-file" {}
    timeout { send_user "unterminated heredoc not detected\n"; exit 1 }
}
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "exit\r"
expect {
    eof {}
    timeout { send_user "eof timeout\n"; exit 1 }
}
