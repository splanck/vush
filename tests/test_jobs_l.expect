#!/usr/bin/env expect
set timeout 5
spawn ../vush
expect "vush> "
send "sleep 5 &\r"
expect "vush> "
send "jobs -l\r"
expect {
    -re "\[1\] [0-9]+ Running sleep 5 &[\r\n]+vush> " {}
    timeout { send_user "jobs -l output mismatch\n"; exit 1 }
}
send "kill 1\r"
expect {
    -re "\[vush\] job [0-9]+ \(sleep 5 \&\) finished[\r\n]+vush> " {}
    timeout { send_user "kill output mismatch\n"; exit 1 }
}
send "exit\r"
expect eof
