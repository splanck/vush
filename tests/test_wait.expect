#!/usr/bin/env expect
set timeout 5
spawn ../vush
expect "vush> "
send "sleep 1 &\r"
expect "vush> "
send "wait\r"
expect "vush> "
send "jobs\r"
expect {
    -re "vush> " {}
    -re "\[1\]" { send_user "job still listed\n"; exit 1 }
    timeout { send_user "jobs timeout\n"; exit 1 }
}
send "exit\r"
expect eof
