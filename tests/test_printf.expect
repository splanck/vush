#!/usr/bin/env expect
set timeout 5
spawn ../vush
expect "vush> "
send "printf \"%s %04d %x\\n\" foo 5 255\r"
expect {
    -re "[\r\n]+foo 0005 ff[\r\n]+vush> " {}
    timeout { send_user "printf output mismatch\n"; exit 1 }
}
send "echo \$?\r"
expect {
    -re "[\r\n]+0[\r\n]+vush> " {}
    timeout { send_user "printf status mismatch\n"; exit 1 }
}
send "exit\r"
expect eof
