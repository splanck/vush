#!/usr/bin/env expect
set timeout 5
spawn ../vush
expect "vush> "
send "foo() { echo hi; }\r"
expect "vush> "
send "foo\r"
expect {
    -re "\[\r\n\]+hi\[\r\n\]+vush> " {}
    timeout { send_user "function execution failed\n"; exit 1 }
}
send "unset -f foo\r"
expect "vush> "
send "foo\r"
expect {
    -re "\[\r\n\]+foo: command not found\[\r\n\]+vush> " {}
    timeout { send_user "function not removed\n"; exit 1 }
}
send "exit\r"
expect eof
