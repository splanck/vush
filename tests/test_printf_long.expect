#!/usr/bin/env expect
set timeout 5
spawn [file dirname [info script]]/../build/vush
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "printf \"%123456789012345678901234567890123d\\n\" 5\r"
expect {
    -re "printf: format specifier too long" {}
    timeout { send_user "missing error\n"; exit 1 }
}
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "echo \$?\r"
expect {
    -re "\[\r\n\]+1\[\r\n\]+vush> " {}
    timeout { send_user "status mismatch\n"; exit 1 }
}
send "exit\r"
expect {
    eof {}
    timeout { send_user "eof timeout\n"; exit 1 }
}
