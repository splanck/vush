#!/usr/bin/env expect
set timeout 5
spawn [file dirname [info script]]/../vush
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "trap 'echo exiting' EXIT\r"
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "exit\r"
expect {
    -re "\[\r\n\]+exiting\[\r\n\]+" {}
    timeout { send_user "EXIT trap output mismatch\n"; exit 1 }
}
expect {
    eof {}
    timeout { send_user "eof timeout\n"; exit 1 }
}
