#!/usr/bin/expect -f
set timeout 5
spawn ../vush
expect "vush> "
# run ls -l on a repository file and check that the filename appears
send "ls -l run_tests.sh\r"
expect {
    -re "[\r\n]+.*run_tests.sh[\r\n]+vush> " {}
    timeout { send_user "ls -l output mismatch\n"; exit 1 }
}
send "exit\r"
expect eof
