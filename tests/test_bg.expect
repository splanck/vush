#!/usr/bin/env expect
set timeout 5
spawn ../vush
expect "vush> "
send "sleep 2 &\r"
expect "vush> "
send "kill -SIGSTOP 1\r"
expect "vush> "
send "bg 1\r"
expect {
    -re "\[vush\] job \[0-9\]+ finished\[\r\n\]+vush> " {}
    timeout { send_user "bg output mismatch\n"; exit 1 }
}
send "exit\r"
expect eof
