#!/usr/bin/env expect
set timeout 5
spawn ../vush
expect "vush> "
send "read first rest\r"
send "one two three\r"
expect "vush> "
send "echo \$first:\$rest\r"
expect {
    -re "[\r\n]+one:two three[\r\n]+vush> " {}
    timeout { send_user "read builtin failed\n"; exit 1 }
}
send "exit\r"
expect eof
