#!/usr/bin/env expect
set timeout 5
spawn ../vush
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "echo \$(echo hi)\r"
expect {
    -re "\[\r\n\]+hi\[\r\n\]+vush> " {}
    timeout { send_user "command substitution failed\n"; exit 1 }
}
send "exit\r"
expect {
    eof {}
    timeout { send_user "eof timeout\n"; exit 1 }
}
