#!/usr/bin/expect -f
set timeout 5
spawn ../vush
expect "vush> "
send "export FOO=bar\r"
expect "vush> "
send "env | grep FOO\r"
expect {
    -re "FOO=bar" {}
    timeout { send_user "export not set\n"; exit 1 }
}
send "export -n FOO\r"
expect "vush> "
send "env | grep FOO\r"
expect {
    -re "\r\nvush> " {}
    timeout { send_user "export -n failed\n"; exit 1 }
}
send "echo $FOO\r"
expect {
    -re "[\r\n]+bar[\r\n]+vush> " {}
    timeout { send_user "export -n removed variable\n"; exit 1 }
}
send "exit\r"
expect eof
