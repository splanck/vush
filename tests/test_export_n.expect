#!/usr/bin/env expect
set timeout 5
spawn [file dirname [info script]]/../vush
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "export FOO=bar\r"
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "env | grep FOO\r"
expect {
    -re "FOO=bar" {}
    timeout { send_user "export not set\n"; exit 1 }
}
send "export -n FOO\r"
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "env | grep FOO\r"
expect {
    -re "\r\nvush> " {}
    timeout { send_user "export -n failed\n"; exit 1 }
}
send "echo \$FOO\r"
expect {
    -re "\[\r\n\]+bar\[\r\n\]+vush> " {}
    timeout { send_user "export -n removed variable\n"; exit 1 }
}
send "exit\r"
expect {
    eof {}
    timeout { send_user "eof timeout\n"; exit 1 }
}
