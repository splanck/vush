#!/usr/bin/env expect
set timeout 5
spawn ../vush
expect "vush> "
send "function kwtest { echo hi \$1; }\r"
expect "vush> "
send "kwtest there\r"
expect {
    -re "[\r\n]+hi there[\r\n]+vush> " {}
    timeout { send_user "function keyword output failed\n"; exit 1 }
}
send "function kv() { return 5; }\r"
expect "vush> "
send "kv\r"
expect "vush> "
send "echo $?\r"
expect {
    -re "[\r\n]+5[\r\n]+vush> " {}
    timeout { send_user "function keyword return failed\n"; exit 1 }
}
send "exit\r"
expect eof
