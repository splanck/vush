#!/usr/bin/env expect
set timeout 5
spawn ../vush
expect {
    "vush> " {}
    timeout { send_user "prompt timeout\n"; exit 1 }
}
send "j=2; until test \$j -eq 0; do echo \$j; j=\$(expr \$j - 1); done\r"
expect {
    -re "2\[\r\n\]+1\[\r\n\]+vush> " {}
    timeout { send_user "until output mismatch\n"; exit 1 }
}
send "exit\r"
expect {
    eof {}
    timeout { send_user "eof timeout\n"; exit 1 }
}
